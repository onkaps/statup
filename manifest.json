{
    "manifest_version": 3,
    "name": "StatUp",
    "description": "A LinkedIn-safe competitive programming stat updates.",
    "version": "0.0.1",
    "permissions": [
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "icons/icon128.png"
        }
    }
}